{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --check \"src/**/*.{ts,tsx,js,css,md,json}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,js,css,md,json}\"",
    "types": "tsc --noEmit",
    "deps:circular": "madge --circular src",
    "deps:unused": "ts-prune",
    "check": "npm-run-all -p lint types format deps:circular deps:unused",
    "fix": "npm-run-all format:fix lint:fix",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "dagre": "^0.8.5",
    "lucide-react": "^0.525.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-flow-renderer": "^10.3.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/dagre": "^0.7.53",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "madge": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.9.2",
    "vite": "^7.0.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,css,md,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
